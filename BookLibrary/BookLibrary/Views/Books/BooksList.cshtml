@model BookLibrary.ViewModels.BooksListBookViewModel
@{
    ViewBag.Title = "Books";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h2>@Model.Book.Name</h2>
@if (Model.Customers.Count == 0)
    {
        <p>No one has rented this book before.</p>
    }
else
{
    <ul>
        @foreach(var customer in Model.Customers)
            {
                <li>@customer.Name</li>
            }
    </ul>
}